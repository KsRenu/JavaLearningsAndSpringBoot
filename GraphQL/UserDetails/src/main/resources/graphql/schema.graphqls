type User{
    id:ID!
    name: String!
    mail: String!
    company: String!
    website: String!
    status : String!
}
type Cred{
    id:ID!
    email: String!
    password: String!
}


type Query{
    users:[User]!
    findUserByStatus(status: String): [User]!
    getUserById(id: String): User!
    findUserByStatusLimit(status: String):[User]!

}


type Mutation{
    addUsers:[User]!
}